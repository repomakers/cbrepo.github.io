	branding: false
});

function saveas(type){
	var input = $('textarea#textbox').val();
	if(type){
	$('#saveas').val(type);
	$('#formSubmit').submit();
	} else {
	alert('Please enter text');
	}
}
	function saveAsPdf(){
		tinyMCE.triggerSave();
		$('#sv_type').val('pdf');
		$('#form').submit();
	}
	function saveAsDoc(){
		tinyMCE.triggerSave();
		$('#sv_type').val('doc');
		$('#form').submit();
	}
</script>		
					
